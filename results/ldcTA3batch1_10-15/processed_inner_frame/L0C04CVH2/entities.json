    },
    "ENL0C04CVH2.000156": {
        "id": "ENL0C04CVH2.000156",
        "type": "Claim_Sentence",
        "offsets": [
            1641,
            1818
        ],
        "text": "Anders Tegnell, the country\u2019s chief epidemiologist and top strategist for the country in the fight against the pandemic, said it \"is much more sustainable\" over a longer period.",
        "arg_ann_id": "T156"
    },
